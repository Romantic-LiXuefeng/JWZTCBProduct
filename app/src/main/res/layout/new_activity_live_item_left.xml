<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/new_bg_background"
    android:layout_width="49dp"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal">

        <ImageView
            android:id="@+id/iv_back"
            android:layout_width="20dp"
            android:layout_height="25dp"
            android:layout_marginTop="13dp"
            />

        <ImageView
            android:id="@+id/iv_voice_open"
            android:layout_below="@id/iv_back"
            android:layout_width="20dp"
            android:layout_height="25dp"
            android:layout_marginTop="37dp"
            android:src="@drawable/new_voice_open"
        />

    <ImageView
        android:id="@+id/iv_shot_change"
        android:layout_below="@id/iv_voice_open"
        android:layout_width="20dp"
        android:layout_height="25dp"
        android:layout_marginTop="37dp"
        android:src="@drawable/new_shot_change"/>

    <ImageView
        android:id="@+id/iv_lamp_closed"
        android:layout_below="@id/iv_shot_change"
        android:layout_width="20dp"
        android:layout_height="25dp"
        android:layout_marginTop="37dp"
        android:src="@drawable/new_lamp_closed"/>

    <ImageView
        android:id="@+id/iv_meiyan_open"
        android:layout_below="@id/iv_lamp_closed"
        android:layout_width="20dp"
        android:layout_height="25dp"
        android:layout_marginTop="37dp"
        android:background="@drawable/new_meiyan_closed"/>

    <ImageView
        android:id="@+id/iv_tpscl_3x"
        android:layout_below="@id/iv_meiyan_open"
        android:layout_width="20dp"
        android:layout_height="25dp"
        android:layout_marginTop="37dp"
        android:src="@drawable/new_tpscl_3x"/>


</RelativeLayout>
