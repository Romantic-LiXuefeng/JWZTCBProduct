<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_subscribe"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:minHeight="38.0dip"
    android:minWidth="72.0dip" >

    
  
    <ImageView 
        android:layout_centerHorizontal="true"
        android:id="@+id/icon_new"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:scaleType="fitXY"
        android:background="@drawable/mine_groupchat_logo"/>
    
    <com.jwzt.caibian.view.FontTextView
        android:id="@+id/text_item"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:layout_below="@id/icon_new"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/video_playter_bg"
        android:textSize="@dimen/main_body_hint_text_size"
        android:text="快速发稿"/>

    

</RelativeLayout>