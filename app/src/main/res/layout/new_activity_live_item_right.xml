<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/new_activity_live_right_item"
    android:layout_width="90dp"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    >

    <ImageView
        android:id="@+id/iv_photo_round"
        android:padding="8dp"
        android:layout_marginTop="17dp"
        android:layout_centerHorizontal="true"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/new_photo"
        android:background="@drawable/new_photo_round"/>

    <ImageView
        android:id="@+id/iv_cemera_round"
        android:padding="8dp"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_centerInParent="true"
        android:src="@drawable/new_camera"
        android:background="@drawable/new_cemera_round"/>

    <ImageView
        android:id="@+id/iv_set_round"
        android:padding="8dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="17dp"
        android:layout_centerHorizontal="true"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/new_set"
        android:background="@drawable/new_set_round"/>
</RelativeLayout>
