<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>

<!--   <corners android:radius="5dip" /> -->
            <gradient
                android:angle="270"
                android:centerColor="@color/seek_grey"
                android:centerY="0.75"
                android:endColor="@color/seek_grey"
                android:startColor="@color/seek_grey" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>

                <!-- <corners android:radius="5dip" /> -->

                <gradient
                    android:angle="270"
                    android:centerColor="@color/seek_grey"
                    android:centerY="0.75"
                    android:endColor="@color/seek_grey"
                    android:startColor="@color/seek_grey" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>

                <!-- <corners android:radius="5dip" /> -->

                <gradient
                    android:angle="270"
                    android:centerColor="@color/seek_blue"
                    android:centerY="0.75"
                    android:endColor="@color/seek_blue"
                    android:startColor="@color/seek_blue" />
            </shape>
        </clip>
    </item>

</layer-list>